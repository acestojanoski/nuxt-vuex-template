    "luxon": "^2.5.2",
    "mongoose": "^5.6.5",
    "nuxt": "^2.8.1",
    "vuex-persist": "^2.0.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
